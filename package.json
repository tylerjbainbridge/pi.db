{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prod-build-next": "next build",
    "prod-start-next": "next start",
    "sync-substack-all": "yarn ts-node ./scraper/controller.ts --all",
    "sync-substack-new": "yarn ts-node ./scraper/controller.ts --new",
    "sync-substack-failed": "yarn ts-node ./scraper/controller.ts --failed",
    "reset-db": "yarn prisma migrate reset && yarn prisma generate",
    "init-db": "yarn prisma migrate && yarn prisma generate",
    "prisma-sync-schema-migration": "yarn prisma db push && yarn prisma generate",
    "prod-build-api": "yarn tsc --resolveJsonModule --esModuleInterop --outDir dist/ api.ts",
    "prod-start-api": "node dist/api.js",
    "dev-start-api": "yarn ts-node api.ts",
    "prod-build": "yarn run prod-build-api && yarn prod-build-next",
    "prod-start": "yarn run prod-start-api && yarn prod-start-next"
  },
  "devDependencies": {
    "babel-plugin-superjson-next": "^0.3.1",
    "prisma": "^2.23.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.6.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^2.23.0",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.6.1",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "commander": "^8.3.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "framer-motion": "^4",
    "lodash": "^4.17.21",
    "next": "10.2.3",
    "puppeteer": "^9.1.1",
    "puppeteer-autoscroll-down": "^0.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "superjson": "^1.7.5"
  }
}
